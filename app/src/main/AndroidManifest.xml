<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.innovationsproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.InnovationsProject">
        <activity android:name=".editables"></activity>
        <activity
            android:name=".directory"
            android:label="@string/title_activity_directory"
            android:theme="@style/Theme.InnovationsProject.NoActionBar" />
        <activity android:name=".settings" />
        <activity android:name=".notes" />
        <activity android:name=".agenda" />
        <activity android:name=".classes" />
        <activity android:name=".announcements" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>